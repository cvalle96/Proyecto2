                <DropShadow />
            </effect>
        </ListView>
        <TextField fx:id="textFieldExpediente" layoutX="558.0" layoutY="151.0" prefHeight="26.0" prefWidth="200.0" promptText="Expediente" />
        <TextField fx:id="textFieldNombre" layoutX="558.0" layoutY="109.0" prefHeight="26.0" prefWidth="200.0" promptText="Nombre" />
        <TextField fx:id="textFieldNota" layoutX="558.0" layoutY="257.0" prefHeight="26.0" prefWidth="200.0" promptText="Nota" />
        <Button fx:id="obtenerButton" alignment="CENTER" layoutX="158.0" layoutY="52.0" mnemonicParsing="false" onAction="#pintarAlumnos" prefHeight="39.0" prefWidth="142.0" text="CARGAR DATOS" textFill="#001cff" />
        <TextField fx:id="textFieldAula" layoutX="558.0" layoutY="194.0" prefHeight="26.0" prefWidth="200.0" promptText="Aula" />
