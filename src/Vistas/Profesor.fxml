                <DropShadow />
            </effect>
        </ListView>
        <TextField fx:id="textFieldComentario" layoutX="558.0" layoutY="297.0" prefHeight="26.0" prefWidth="200.0" promptText="Comentario" />
        <Button fx:id="buttonEnviarNota" layoutX="566.0" layoutY="359.0" mnemonicParsing="false" onAction="#enviaNota" prefHeight="38.0" prefWidth="184.0" text="ENVIAR NOTA" textFill="#3fa255">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
    </children>
</AnchorPane>