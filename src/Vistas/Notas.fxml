<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="ap" prefHeight="592.0" prefWidth="824.0" styleClass="background" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controladoras.controladoraNotas">

    <children>
        <ListView fx:id="notasListview" layoutX="37.0" layoutY="168.0" prefHeight="287.0" prefWidth="729.0">

            <effect>
                <DropShadow />
            </effect>
            <opaqueInsets>
                <Insets left="2.0" top="2.0" />
            </opaqueInsets>
        </ListView>
        <Label fx:id="tituloNotas" layoutX="92.0" layoutY="57.0" prefHeight="74.0" prefWidth="159.0" style="-fx-font-size: 3em;" text="NOTAS">
            <font>
                <Font size="36.0" />
            </font>
        </Label>
        <Label fx:id="actualizadoNotas" layoutX="402.0" layoutY="57.0" prefHeight="74.0" prefWidth="179.0" text="ACTUALIZADO:">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <Label fx:id="labelHora" alignment="CENTER" layoutX="613.0" layoutY="78.0" prefHeight="32.0" prefWidth="110.0" text="Hora">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <Button fx:id="actualizarButton" alignment="CENTER" layoutX="513.0" layoutY="486.0" mnemonicParsing="false" onAction="#actualizar" prefHeight="35.0" prefWidth="179.0" text="ACTUALIZAR" textFill="#4431f5">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
