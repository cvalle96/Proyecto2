<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="ap" prefHeight="592.0" prefWidth="824.0" styleClass="background" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controladoras.controladoraCalificar">

    <children>
        <ListView fx:id="listaUsuarios" layoutX="35.0" layoutY="134.0" onMouseClicked="#selectThisUser" prefHeight="325.0" prefWidth="184.0">
            <effect>
                <DropShadow />
            </effect>
        </ListView>
        <TextField fx:id="textFieldNota" layoutX="558.0" layoutY="191.0" prefHeight="26.0" prefWidth="200.0" promptText="Nota" style="-fx-border-color: #3fa255;" />
        <Button fx:id="obtenerButton" alignment="CENTER" layoutX="158.0" layoutY="52.0" mnemonicParsing="false" onAction="#pintarAlumnos" prefHeight="39.0" prefWidth="142.0" text="CARGAR DATOS" textFill="#001cff" />
        <ListView fx:id="listaAsignatura" layoutX="237.0" layoutY="134.0" onMouseClicked="#selectThisAsignatura" prefHeight="325.0" prefWidth="184.0">
            <effect>
                <DropShadow />
            </effect>
        </ListView>
        <TextField fx:id="textFieldComentario" layoutX="558.0" layoutY="269.0" prefHeight="26.0" prefWidth="200.0" promptText="Comentario" style="-fx-border-color: #3fa255;" />
        <Button fx:id="buttonEnviarNota" layoutX="566.0" layoutY="359.0" mnemonicParsing="false" onAction="#enviaNota" prefHeight="38.0" prefWidth="184.0" text="ENVIAR NOTA" textFill="#3fa255">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
