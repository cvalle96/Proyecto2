<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="592.0" prefWidth="824.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controladoras.controladoraEditor">

    <children>
        <ListView fx:id="listaUsuarios" layoutX="42.0" layoutY="125.0" onMouseClicked="#selectThisUser" prefHeight="321.0" prefWidth="332.0">
            <effect>
                <DropShadow />
            </effect>
        </ListView>
        <Label layoutX="84.0" layoutY="89.0" prefHeight="18.0" prefWidth="166.0" text="Selecciona usuario de tu clase" />
        <Label fx:id="labelAlumnoModificar" alignment="CENTER" contentDisplay="CENTER" layoutX="476.0" layoutY="83.0" prefHeight="48.0" prefWidth="200.0" textFill="#3d10f0">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <TextField fx:id="textFieldExpediente" layoutX="476.0" layoutY="214.0" prefHeight="26.0" prefWidth="200.0" promptText="Expediente" />
        <TextField fx:id="textFieldNombre" layoutX="476.0" layoutY="107.0" prefHeight="26.0" prefWidth="200.0" promptText="Nombre" />
        <Button fx:id="actualizarButton" layoutX="484.0" layoutY="330.0" mnemonicParsing="false" onAction="#modificarValores" prefHeight="38.0" prefWidth="184.0" style="-fx-background-color: #2AFF00;" text="ENVIAR CAMBIOS" />
        <Button fx:id="borrarButton" layoutX="484.0" layoutY="391.0" mnemonicParsing="false" onAction="#borrarEsteUsuario" prefHeight="38.0" prefWidth="184.0" text="BORRAR USUARIO" textFill="#ea0101" />
        <TextField fx:id="textFieldGrupo" layoutX="476.0" layoutY="269.0" prefHeight="26.0" prefWidth="200.0" promptText="Clase" />
        <TextField fx:id="textFieldApellidos" layoutX="476.0" layoutY="160.0" prefHeight="26.0" prefWidth="200.0" promptText="Apellidos" />
    </children>
</AnchorPane>
